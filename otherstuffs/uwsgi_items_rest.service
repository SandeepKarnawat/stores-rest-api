[Unit]
Description=uWSGI items rest

[Service]
Environment=DATABASE_URL=postgres://skarnawat:<passwd>@localhost:5432/skarnawat
ExecStart=/var/www/html/items-rest/venv/bin/uwsgi --master --emperor /var/www/html/items-rest/uwsgi.ini --die-on-term --uid skarnawat --gid skarnawat --logto /var/www/html/items-rest/log/emperor.log
Restart=always
KillSignal=SIGQUIT
Type=notify
NotifyAccess=all

[Install]
WantedBy=multi-user.target

